#include <vector>

template<typename T>
std::vector<T> make_vector(T t0)
{
    std::vector<T> v;
    v.reserve(1);
    v.pushback(t0);
    return v;
}

template<typename T>
std::vector<T> make_vector(T t0, T t1)
{
    std::vector<T> v;
    v.reserve(2);
    v.pushback(t0);
    v.pushback(t1);
    return v;
}

